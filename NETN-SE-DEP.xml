<?xml version="1.0" encoding="UTF-8"?>

<module name="NETN-CBRN" file="./NETN-CBRN/NETN-CBRN.xml">
    <objectClass name="HLAobjectRoot">
        <attribute required="true" inherited="true" module="NETN-BASE" class="HLAobjectRoot">UniqueId</attribute>
        <objectClass name="EnvironmentObject">
            <attribute required="true" inherited="true" module="RPR-SE" class="EnvironmentObject">ObjectIdentifier</attribute>
            <attribute required="true" inherited="true" module="RPR-SE" class="EnvironmentObject">ObjectType</attribute>
            <attribute required="true" inherited="true" module="RPR-SE" class="EnvironmentObject">ForceIdentifier</attribute>
            <objectClass name="ArealObject">
                <attribute required="true" inherited="true" module="RPR-SE" class="ArealObject">PointsData</attribute>
             
            </objectClass>
            <objectClass name="PointObject">
                <attribute required="true" inherited="true" module="RPR-SE" class="PointObject">Location</attribute>
                <attribute required="true" inherited="true" module="RPR-SE" class="PointObject">Orientation</attribute>
               
            </objectClass>
        </objectClass>
    </objectClass>
    <interactionClass name="HLAinteractionRoot">
        <parameter required="true" module="NETN-BASE" class="HLAinteractionRoot" post=" Required for all ETR related interactions.">UniqueId</parameter>
   
        <interactionClass name="SMC_EntityControl">
            <parameter required="true" module="NETN-SMC" class="SMC_EntityControl" post=" Required for all ETR related interactions.">Entity</parameter>
            <interactionClass name="Task">
                <parameter required="true" module="NETN-ETR" class="Task">TaskId</parameter>
                <interactionClass name="CreateBreach" hide="true"/>
                <interactionClass name="EstablishCheckpoint" hide="true"/>
                <interactionClass name="LayMinefield" hide="true"/>
                <interactionClass name="CreateObstacle" hide="true"/>
                <interactionClass name="ClearEngineering" hide="true"/>
                
                <interactionClass name="EstablishObervationPost" hide="true"/>
             
                
            </interactionClass>
            <interactionClass name="RequestTaskStatus">
            </interactionClass>
            <interactionClass name="CancelTasks">
            </interactionClass>
        </interactionClass>
        
       
    </interactionClass>
    
</module>